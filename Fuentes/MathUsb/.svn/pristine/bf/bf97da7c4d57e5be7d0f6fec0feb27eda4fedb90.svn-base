<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets">

	<h:head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
	</h:head>

	<h:body>
	    <ui:composition template="/WEB-INF/facelets/template.xhtml">	
		 
		 	<ui:define name="body">	
				<f:view>
					<p:ajaxStatus onstart="PF('statusDialog').show()" onsuccess="PF('statusDialog').hide()"/>               
					<p:dialog modal="true" widgetVar="statusDialog" header="Loading"  draggable="false" closable="false">  
   						<p:graphicImage value="#{facesContext.externalContext.request.scheme}://#{facesContext.externalContext.request.serverName}:#{facesContext.externalContext.request.serverPort}#{facesContext.externalContext.request.contextPath}/images/ajaxloadingbar.gif" />   
					</p:dialog>
					
					
					<!-- Expired session -->
					<p:idleMonitor onactive="PF('sessionTimeOutDialog').show();" timeout="#{session.maxInactiveInterval * 1000}" />
					<p:dialog modal="true" widgetVar="sessionTimeOutDialog" header="Expired session"  draggable="false" closable="false">  
	   					<h:outputText value="The session is finished" />
	                 	<input id="confirmBackToLogin" 
	                 		type="button" 
	                 		value="Sign in"
	                    	onclick="window.location='#{facesContext.externalContext.requestContextPath}/index.jsp';" />   
					</p:dialog>
		 
		 <h:form id="form">
			
			 
			<p:panel id="msgProjectClientesTitle" header="DetalleEvaluacionData">
					<p:messages id="msg" />
				<p:dataTable id ="tabla" value="#{detalleEvaluacionView.data}" var="detalleEvaluacion"
					paginator="true" rows="5"  editable="true" >
					 
					 <p:ajax event ="rowEdit" update=":form:msg,:form:tabla" listener="#{detalleEvaluacionView.rowEventListener}" />
					
															<p:column filterBy="#{detalleEvaluacion.deevId}" sortBy="#{detalleEvaluacion.deevId}" headerText="deevId">
						<p:cellEditor>
							<f:facet name="output">
								<h:outputText value="#{detalleEvaluacion.deevId}"/>
							</f:facet>
							<f:facet name="input">
								<p:inputText value="#{detalleEvaluacion.deevId}"/>
							</f:facet>
						</p:cellEditor>
					</p:column>
                    										
											
						<p:column filterBy="#{detalleEvaluacion.asunto}" sortBy="#{detalleEvaluacion.asunto}" 
							headerText="asunto">
							
								<p:cellEditor>
								
								<f:facet name="output">
									<h:outputText value="#{detalleEvaluacion.asunto}"/>
								</f:facet>
							
								<f:facet name="input">
									<p:inputText value="#{detalleEvaluacion.asunto}"/>
								</f:facet>
								</p:cellEditor>	
							</p:column>
						
											
						<p:column filterBy="#{detalleEvaluacion.corte}" sortBy="#{detalleEvaluacion.corte}" 
							headerText="corte">
							
								<p:cellEditor>
								
								<f:facet name="output">
									<h:outputText value="#{detalleEvaluacion.corte}"/>
								</f:facet>
							
								<f:facet name="input">
									<p:inputText value="#{detalleEvaluacion.corte}"/>
								</f:facet>
								</p:cellEditor>	
							</p:column>
						
											
						<p:column filterBy="#{detalleEvaluacion.evalId_Evaluacion}" sortBy="#{detalleEvaluacion.evalId_Evaluacion}" 
							headerText="evalId_Evaluacion">
							
								<p:cellEditor>
								
								<f:facet name="output">
									<h:outputText value="#{detalleEvaluacion.evalId_Evaluacion}"/>
								</f:facet>
							
								<f:facet name="input">
									<p:inputText value="#{detalleEvaluacion.evalId_Evaluacion}"/>
								</f:facet>
								</p:cellEditor>	
							</p:column>
						
										
										   						 
						 <p:column filterBy="#{detalleEvaluacion.fechaCreacion}" sortBy="#{detalleEvaluacion.fechaCreacion}" 
							headerText="fechaCreacion">
							
								<p:cellEditor>
								
								<f:facet name="output">
									<h:outputText value="#{detalleEvaluacion.fechaCreacion}">
									  <f:convertDateTime  pattern="dd/MM/yyyy"/>
									</h:outputText>		
								</f:facet>
							
								<f:facet name="input">
									<p:calendar value="#{detalleEvaluacion.fechaCreacion}" showOn="button" pattern="dd/MM/yyyy" navigator="true" >
									  <f:convertDateTime  pattern="dd/MM/yyyy" timeZone="#{detalleEvaluacionView.timeZone}" />	
								    </p:calendar>		
								</f:facet>
								</p:cellEditor>	
							</p:column>
						
					   						 
						 <p:column filterBy="#{detalleEvaluacion.fechaFinPublicacion}" sortBy="#{detalleEvaluacion.fechaFinPublicacion}" 
							headerText="fechaFinPublicacion">
							
								<p:cellEditor>
								
								<f:facet name="output">
									<h:outputText value="#{detalleEvaluacion.fechaFinPublicacion}">
									  <f:convertDateTime  pattern="dd/MM/yyyy"/>
									</h:outputText>		
								</f:facet>
							
								<f:facet name="input">
									<p:calendar value="#{detalleEvaluacion.fechaFinPublicacion}" showOn="button" pattern="dd/MM/yyyy" navigator="true" >
									  <f:convertDateTime  pattern="dd/MM/yyyy" timeZone="#{detalleEvaluacionView.timeZone}" />	
								    </p:calendar>		
								</f:facet>
								</p:cellEditor>	
							</p:column>
						
					   						 
						 <p:column filterBy="#{detalleEvaluacion.fechaInicioPublicacion}" sortBy="#{detalleEvaluacion.fechaInicioPublicacion}" 
							headerText="fechaInicioPublicacion">
							
								<p:cellEditor>
								
								<f:facet name="output">
									<h:outputText value="#{detalleEvaluacion.fechaInicioPublicacion}">
									  <f:convertDateTime  pattern="dd/MM/yyyy"/>
									</h:outputText>		
								</f:facet>
							
								<f:facet name="input">
									<p:calendar value="#{detalleEvaluacion.fechaInicioPublicacion}" showOn="button" pattern="dd/MM/yyyy" navigator="true" >
									  <f:convertDateTime  pattern="dd/MM/yyyy" timeZone="#{detalleEvaluacionView.timeZone}" />	
								    </p:calendar>		
								</f:facet>
								</p:cellEditor>	
							</p:column>
						
					   											
					<p:column headerText="Options" >
						<p:rowEditor id="row"/>
						<p:tooltip for="row" value="Edit" showEffect="fade" hideEffect="fade" />
						<p:tooltip for="btnDelete" value="Delete" showEffect="fade" hideEffect="fade" />
						<p:commandButton id="btnDelete" actionListener="#{detalleEvaluacionView.actionDeleteDataTableEditable}" title="Delete" onclick="if(!confirm('Do you really want to delete this Entry?')){return false;}" icon="ui-icon-trash" update=":form:msg,:form:tabla" >
							<f:attribute name="selectedDetalleEvaluacion" value="#{detalleEvaluacion}" />  
						</p:commandButton>
						
					</p:column>	
				
				</p:dataTable>	
					
				
			</p:panel>
				
	   </h:form>
		 </f:view>
			</ui:define>		
		</ui:composition>	
	
	</h:body>	
	
</html>	