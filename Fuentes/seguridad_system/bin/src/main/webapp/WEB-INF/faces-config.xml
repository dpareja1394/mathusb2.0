 <navigation-rule>
  <from-view-id>/login2.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>goInitialMenu</from-outcome>
   <to-view-id>/XHTML/initialMenu.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goSeleccionarSisYCias</from-outcome>
   <to-view-id>/XHTML/selectSisYCias.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/XHTML/selectSisYCias.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>goInitialMenu</from-outcome>
   <to-view-id>/XHTML/initialMenu.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>goRol</from-outcome>
   <to-view-id>/XHTML/segRol.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goSistema</from-outcome>
   <to-view-id>/XHTML/segSistema.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goSucursal</from-outcome>
   <to-view-id>/XHTML/segSucursal.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goUsuario</from-outcome>
   <to-view-id>/XHTML/segUsuario.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goCompania</from-outcome>
   <to-view-id>/XHTML/segCompania.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goMenu</from-outcome>
   <to-view-id>/XHTML/initialMenu.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goRolUsuario</from-outcome>
   <to-view-id>/XHTML/segRolUsuario.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goGrupoOpcion</from-outcome>
   <to-view-id>/XHTML/segGrupoOpcion.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goOpcion</from-outcome>
   <to-view-id>/XHTML/segOpcion.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goOpcionRol</from-outcome>
   <to-view-id>/XHTML/tree.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goAuditoria</from-outcome>
   <to-view-id>/XHTML/segAuditoria.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goSistemaCompania</from-outcome>
   <to-view-id>/XHTML/segSistemaCia.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goSalir</from-outcome>
   <to-view-id>/login2.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goParametros</from-outcome>
   <to-view-id>/XHTML/segParametro.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goPermisos</from-outcome>
   <to-view-id>/XHTML/segPermiso.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>goInicio</from-outcome>
   <to-view-id>/XHTML/selectSisYCias.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 
 <lifecycle>
    <phase-listener id="nocache">com.vortexbird.seguridad.utilities.CacheControlPhaseListener</phase-listener>
</lifecycle>

</faces-config>
