							<h:outputText value="UsuCodigo_SegUsuario"/>
							</f:facet>
							 <h:outputText value="#{segHistorialConstrasena.usuCodigo_SegUsuario}"/>
							</p:column>   
                    						
					
				
				</p:dataTable>	
					
				
			</p:panel>
				
