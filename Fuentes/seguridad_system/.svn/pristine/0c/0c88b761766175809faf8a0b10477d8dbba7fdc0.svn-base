							</f:facet>
							 <h:outputText value="#{segHistorialConstrasena.hcoConstrasena}"/>
							</p:column>   
                    						
							<p:column filterBy="#{segHistorialConstrasena.hcoDetalleCambio}" sortBy="#{segHistorialConstrasena.hcoDetalleCambio}" >
