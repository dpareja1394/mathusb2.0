        <br/>
        <br/> 	
	   <ui:define name="body">	
		 <f:view>
	   <h:form>
			<div align="center">
               <h:link id="cmdBack" outcome="goInitialMenu" value="Back" style="color:blue;" />
			 </div>
			 <br />
			 
			<p:panel id="msgProjectClientesTitle" header="SegHistorialConstrasenaData">
				
				<p:dataTable value="#{segHistorialConstrasenaView.data}" var="segHistorialConstrasena"
					paginator="true" rows="5">
																	<p:column filterBy="#{segHistorialConstrasena.hcoCodigo}" sortBy="#{segHistorialConstrasena.hcoCodigo}" >
							 <f:facet name="header">
							<h:outputText value="hcoCodigo"/>
							</f:facet>
							 <h:outputText value="#{segHistorialConstrasena.hcoCodigo}"/>
							 </p:column>
                    																
							<p:column filterBy="#{segHistorialConstrasena.hcoConstrasena}" sortBy="#{segHistorialConstrasena.hcoConstrasena}" >
							 <f:facet name="header">
